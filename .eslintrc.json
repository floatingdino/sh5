{
  "extends": "airbnb-base",
  "plugins": ["html"],
  "env": {
    "browser": true
  },
  "globals": {
    "ga": false
  },
  "rules": {
    "quotes": ["warn", "double"],
    "no-console": "off",
    "import/extensions": "never"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts", ".vue"]
      }
    }
  }
}
